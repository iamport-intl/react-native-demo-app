    "lodash": "^4.17.21",
    "query-string": "^6.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-custom-webview": "^1.0.37",
    "react-native-elements": "^3.3.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-otp-textinput": "^0.0.8",
