    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "accordion-collapse-react-native": "^1.0.1",
    "axios": "^0.21.1",
    "chaipay-sdk": "file:paymentSDK",
    "crypto-js": "^4.1.1",
